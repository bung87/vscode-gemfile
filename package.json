{
    "name": "vscode-gemfile",
    "displayName": "vscode-gemfile",
    "description": "provide hover link in Gemfile refers to online site",
    "version": "0.4.1",
    "publisher": "bung87",
    "engines": {
        "vscode": "^1.43.0"
    },
    "repository": {
        "url": "https://github.com/bung87/vscode-gemfile"
    },
    "icon": "Gemfile.png",
    "categories": [
        "Other"
    ],
