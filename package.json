        "compile": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/mocha": "^7.0.2",
        "@types/node": "^13.9.1",
        "@types/vscode": "^1.43.0",
        "mocha": "^2.3.3",
        "typescript": "^3.8.3",
        "vscode-test": "^1.3.0"
    },
    "dependencies": {
        "gemfile": "^1.1.0"
    }
}