{
    "name": "vscode-gemfile",
    "displayName": "vscode-gemfile",
    "description": "",
    "version": "0.0.1",
    "publisher": "周鹏",
    "engines": {
        "vscode": "^1.23.0"
    },
    "enableProposedApi": true,
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "workspaceContains:Gemfile.lock"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "Gemfile:Document",
                "title": "Gem Document"
            }
        ],
        "keybindings": [
            {
                "command": "Gemfile:Document",
                "key": "Alt+F1"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "*",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {
        "gemfile": "^1.0.0",
        "he": "^1.1.1",
        "request-promise-native": "^1.0.5",
        "sanitize-html": "^1.18.2"
    }
}
