{
    "name": "vscode-gemfile",
    "displayName": "vscode-gemfile",
    "description": "provide hover link in Gemfile refers to online site",
    "version": "0.0.1",
    "publisher": "bung87",
    "engines": {
        "vscode": "^1.23.0"
    },
    "repository": {
        "url": "https://github.com/bung87/vscode-gemfile"
    },
    "icon": "Gemfile.png",
    "categories": [
        "Other"
    ],
    "galleryBanner": {
        "color": "#c52f24",
        "theme": "dark"
    },
    "activationEvents": [
        "workspaceContains:Gemfile.lock"
    ],
    "main": "./out/src/extension",
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "*",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {
        "vscode": "^1.1.18",
        "gemfile": "^1.0.0"
    }
}
